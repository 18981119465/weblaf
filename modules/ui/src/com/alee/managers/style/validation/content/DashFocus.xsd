<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <xs:include schemaLocation="AbstractContent.xsd" />
    <xs:include schemaLocation="../awt/Stroke.xsd" />
    <xs:include schemaLocation="../awt/Color.xsd" />

    <!-- com.alee.painter.decoration.content.DashFocus -->
    <xs:complexType name="DashFocus">
        <xs:complexContent>
            <xs:extension base="AbstractContent">

                <!-- Focus rounding -->
                <xs:attribute name="round" type="xs:integer" />

                <!-- Focus stroke -->
                <xs:attribute name="stroke" type="Stroke" />

                <!-- Focus color -->
                <xs:attribute name="color" type="Color" />

            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

</xs:schema>
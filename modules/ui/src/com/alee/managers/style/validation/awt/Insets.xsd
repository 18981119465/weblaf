<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema targetNamespace="http://weblookandfeel.com" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

    <!-- java.awt.Insets -->
    <xs:simpleType name="Insets">
        <xs:restriction base="xs:string">

            <!-- Simple insets -->
            <xs:pattern value="^[0-9]+$" />

            <!-- Full insets -->
            <xs:pattern value="^[0-9]+,[0-9]+,[0-9]+,[0-9]+$" />

        </xs:restriction>
    </xs:simpleType>

</xs:schema>
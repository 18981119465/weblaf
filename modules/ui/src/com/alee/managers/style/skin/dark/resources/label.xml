<skin>

    <!-- com.alee.laf.label.WebLabel -->

    <!-- Label -->
    <style type="label">
        <component>
            <opaque>false</opaque>
            <foreground>white</foreground>
            <background>237,237,237</background>
        </component>
        <painter class="com.alee.managers.style.skin.web.WebLabelPainter">
            <drawShade>false</drawShade>
            <shadeColor>0,0,0</shadeColor>
            <shadeSize>2</shadeSize>
            <rotation>none</rotation>
        </painter>
    </style>

    <!-- Label with tag decoration -->
    <style type="label" id="tag">
        <painter class="com.alee.managers.style.skin.web.TagLabelPainter">
            <border>65,70,70</border>
            <background>80,85,90</background>
        </painter>
    </style>

</skin>
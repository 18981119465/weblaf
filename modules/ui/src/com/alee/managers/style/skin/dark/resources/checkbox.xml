<skin>

    <!-- com.alee.laf.checkbox.WebCheckBox -->

    <!-- Checkbox -->
    <style type="checkbox">
        <component>
            <foreground>255,255,255,240</foreground>
        </component>
        <painter>
            <selectedForeground>255,255,255,240</selectedForeground>

            <!-- Check state painter -->
            <checkStatePainter>
                <decorations>
                    <checkDecoration size="17,17">
                        <WebShape round="2" />
                        <WebShadow type="outer" width="2" />
                        <LineBorder color="10,10,10" />
                        <GradientBackground type="radial" from="0.5,0.5" to="2,0.5">
                            <color>77,81,83</color>
                            <color>black</color>
                        </GradientBackground>
                    </checkDecoration>
                    <checkDecoration states="checked">
                        <CheckIcon stroke="basic;2" color="255,255,255,235" />
                    </checkDecoration>
                    <checkDecoration states="focused">
                        <LineBorder color="black" />
                    </checkDecoration>
                    <checkDecoration states="disabled">
                        <WebShadow type="outer" opacity="0" />
                        <LineBorder color="68,72,74" />
                        <GradientBackground type="radial" from="0.5,0.5" to="2,0.5">
                            <color>111,115,117</color>
                            <color>77,81,83</color>
                        </GradientBackground>
                    </checkDecoration>
                    <checkDecoration states="disabled,checked">
                        <CheckIcon color="130,130,130" />
                    </checkDecoration>
                </decorations>
            </checkStatePainter>

        </painter>
    </style>

</skin>
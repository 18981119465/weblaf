<skin>

    <!-- com.alee.extended.list.WebFileList -->

    <!-- File list -->
    <style type="list" id="filelist">

        <!-- Cell renderer -->
        <style type="label" id="renderer" />

        <!-- Tile cell renderer -->
        <style type="label" id="tile-renderer" extends="renderer" padding="6,6,5,8" />

        <!-- Icon cell renderer -->
        <style type="label" id="icon-renderer" extends="renderer" padding="5,5,8,5" />

        <!-- Cell editor field -->
        <style type="textField" id="editor" extends="unfocusable" />

    </style>

</skin>